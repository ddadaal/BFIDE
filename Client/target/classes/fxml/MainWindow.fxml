<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="535.0" prefWidth="611.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viccrubs.bfide.client.controllers.MainWindowController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="608.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#openNewDialog" text="New Project" />
                  <MenuItem mnemonicParsing="false" onAction="#openOpenDialog" text="Open Project from Server" />
                  <MenuItem mnemonicParsing="false" onAction="#save" text="Save" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu fx:id="menuVersionControl" mnemonicParsing="false" text="Version Control">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#openAbout" text="About" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
      <AnchorPane layoutY="308.0" prefHeight="208.0" prefWidth="611.0">
         <children>
            <Text layoutX="429.0" layoutY="25.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Output" />
            <TextArea fx:id="textInput" layoutY="40.0" prefHeight="129.0" prefWidth="300.0" />
            <TextArea fx:id="textOutput" editable="false" layoutX="308.0" layoutY="40.0" prefHeight="129.0" prefWidth="300.0" />
            <Text layoutX="129.0" layoutY="24.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Input" />
         </children>
      </AnchorPane>
      <AnchorPane layoutY="25.0" prefHeight="35.0" prefWidth="608.0">
         <children>
            <Button fx:id="btnRun" layoutX="8.0" layoutY="6.0" mnemonicParsing="false" onMouseClicked="#onRunClicked" text="Run" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
            <MenuButton fx:id="userButton" layoutX="503.0" layoutY="3.0" mnemonicParsing="false" text="Username" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#logout" text="Logout" />
              </items>
            </MenuButton>
            <Button fx:id="btnRunWithoutInput" layoutX="58.0" layoutY="6.0" mnemonicParsing="false" onMouseClicked="#onRunWithoutInputClicked" text="Run without Input" AnchorPane.bottomAnchor="5.0" AnchorPane.topAnchor="5.0" />
            <Text fx:id="textCurrentProject" layoutX="218.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
      </AnchorPane>
      <AnchorPane layoutY="548.0" prefHeight="25.0" prefWidth="611.0" AnchorPane.bottomAnchor="0.0">
         <children>
            <Text fx:id="textStatus" layoutX="8.0" layoutY="17.0" onMouseClicked="#showLogs" strokeType="OUTSIDE" strokeWidth="0.0" text="Ready" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" />
            <Text fx:id="textLanguage" layoutX="542.0" layoutY="17.0" onMouseClicked="#switchLanguage" strokeType="OUTSIDE" strokeWidth="0.0" text="Language" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" />
         </children>
      </AnchorPane>
      <TextArea fx:id="textCode" layoutY="60.0" prefHeight="248.0" prefWidth="611.0" promptText="Code here" wrapText="true" />
   </children>
</AnchorPane>
